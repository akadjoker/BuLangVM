
var arr = [1, 2, 3, 4, 5];

arr.remove(2);        // [1, 2, 4, 5]

print(arr);


arr.insert(1, 99);    // [1, 99, 2, 4, 5]

print(arr);

print(arr.find(99));         // 1
print(arr.contains(99));     // true
arr.reverse();        // [5, 4, 2, 99, 1]

var slice = arr.slice(1, 3);  // [4, 2]
print(slice);


var concat = arr.concat([10, 20]); // [5, 4, 2, 99, 1, 10, 20]
print(concat);

print(arr.first());  // 5
print(arr.last());   // 1
print(arr.fill(0));  // [0, 0, 0, 0, 0]