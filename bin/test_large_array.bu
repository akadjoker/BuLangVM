// Test large arrays (>255 elements) using dynamic generation
// Note: Inline literals [1,2,3,...] are limited by constants table (255 max)
// But arrays built dynamically can be much larger

// Generate an array with 500 elements
var arr = [];
for (var i = 0; i < 500; i = i + 1) {
    arr.push(i);
}

print("Array length: " + len(arr));
print("First element: " + arr[0]);
print("Element 100: " + arr[100]);
print("Element 255: " + arr[255]);
print("Element 300: " + arr[300]);
print("Last element: " + arr[499]);

// Verify values
var pass = true;
for (var i = 0; i < 500; i = i + 1) {
    if (arr[i] != i) {
        print("FAIL at index " + i + ": expected " + i + " got " + arr[i]);
        pass = false;
    }
}

if (pass) {
    print("PASS: All 500 elements correct!");
}

