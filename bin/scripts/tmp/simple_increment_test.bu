// Teste simples para debug
print("Teste 1: Postfix básico");
var i = 5;
print("i antes: " + i);
var x = i++;
print("x (deve ser 5): " + x);
print("i (deve ser 6): " + i);
print("");

print("Teste 2: Prefix básico");
var j = 5;
print("j antes: " + j);
var y = ++j;
print("y (deve ser 6): " + y);
print("j (deve ser 6): " + j);
